<%- include("partials/header"); -%>
<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="composeTitle">Title</label>
    <input type="text" name="postTitle" class="form-control">
  </div>
  <div class="form-group">
    <label for="composeBody">Post</label>
    <textarea name="postBody" rows="5" cols="30" class="form-control"></textarea>
  </div>
  <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>
<%- include("partials/footer"); -%>
